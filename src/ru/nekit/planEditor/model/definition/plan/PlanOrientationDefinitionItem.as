package ru.nekit.planEditor.model.definition.plan
{
	public class PlanOrientationDefinitionItem
	{
		
		public var id:String;
		public var label:String;
		public var width:Number;
		public var height:Number;
		
		public function PlanOrientationDefinitionItem(id:String, label:String, width:Number, height:Number)
		{
			this.id			= id;
			this.label		= label;
			this.width 	= width;
			this.height 	= height;
		}
	}
}
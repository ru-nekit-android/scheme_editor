package ru.nekit.planEditor.model.vo
{
	
	[Bindable]
	[RemoteClass(alias="BuildingVO")]
	public class BuildingVO
	{
		
		public var id:uint = 0;
		public var number:String = null;
		public var empty:Boolean = true;
	
		public function set(value:BuildingVO):void
		{
			id 			= value.id;
			number= value.number;
		}
		
		public function clear():void
		{
			id = 0;
			number = null;
		}
		
	}
	
}
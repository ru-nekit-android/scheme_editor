package ru.nekit.planEditor.model.vo
{
	
	[Bindable]
	[RemoteClass(alias="StreetVO")]
	public class StreetVO
	{
		
		public var id:uint;
		public var name:String;
		
		public function set(value:StreetVO):void
		{
			id 			= value.id;
			name 	= value.name;
		}
		
		public function clear():void
		{
			id = 0;
			name = null;
		}
	}
}